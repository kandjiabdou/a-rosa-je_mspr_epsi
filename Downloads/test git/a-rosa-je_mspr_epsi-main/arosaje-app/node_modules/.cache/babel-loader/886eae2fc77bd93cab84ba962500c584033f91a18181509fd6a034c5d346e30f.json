             :src=\"`https://picsum.photos/500/300?image=${n * 5 + 10}`\"\n                    :lazy-src=\"`https://picsum.photos/10/6?image=${n * 5 + 10}`\"\n                    aspect-ratio=\"1\"\n                    cover\n                    class=\"bg-grey-lighten-2\"\n                  >\n                    <template v-slot:placeholder>\n                      <v-row\n                        class=\"fill-height ma-0\"\n                        text-align=\"center\"\n                        justify=\"center\"\n                      >\n                        <v-progress-circular\n                          indeterminate\n                          color=\"grey-lighten-5\"\n                        ></v-progress-circular>\n                      </v-row>\n                    </template>\n                  </v-img>\n                </v-col>\n              </v-row>\n            </v-card-text>\n          </v-card>\n        </v-window-item>\n      </v-window>\n    </div>\n  </v-card>\n</template>\n\n<script>\nimport liste from \"../data.json\";\nimport { mdiDelete } from \"@mdi/js\";\nimport axios from \"axios\";\n\nexport default {\n  name: \"liSte\",\n\n  data: () => ({\n    tab: 0,\n    items: liste[\"liste\"],\n    icon: mdiDelete,\n    gardiennageData: [],\n  }),\n  methods: {\n    deleteItem(index) {\n      this.items.splice(index, 1);\n    },\n    fetchData() {\n      console.log(\"test start\");\n      axios\n        .get(\"http://localhost:3000/gardiennage\")\n        .then((response) => {\n          this.GardiennageData = response.data;\n          console.log(\"ok get\");\n          console.log(response);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      console.log(\"test fin\");\n\n      // axios.get('http://localhost:3000/gardiennage')\n      //   .then(response => {\n      //     this.gardiennageData = response.data;\n      //     console.log(response.data);\n      //   })\n      //   .catch(error => {\n      //     console.log(error);\n      //   });\n      // const axios = require('axios');\n\n      // let res = await axios.get('http://localhost:3000/gardiennage');\n\n      // let data = res.data;\n      // console.log(data);\n    },\n  },\n};\n</script>\n\n<style>\n/* Sets the height of the tabs to 100% of their parent element */\n.v-tabs__content {\n  height: 100%;\n}\n\n/* Adds a border and margin to the tab items */\n.v-tab {\n  border: 1px solid #ccc;\n  margin-bottom: 10px;\n}\n\n/* Changes the font color and size of the tab items */\n.v-tab__wrapper {\n  color: #333;\n  font-size: 16px;\n}\n\n/* Add padding and background color to the tab items */\n.v-tab__item {\n  padding: 10px;\n  background-color: #f5f5f5;\n}\n\n/* Changes the font color and size of the toolbar title */\n.v-toolbar__title {\n  color: #fff;\n  font-size: 24px;\n}\n\n/* Adds some padding to the card text */\n.v-card-text {\n  padding: 10px;\n}\n\n/* Centers the images within the card text */\n.v-card-text img {\n  display: block;\n  margin: 0 auto;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}