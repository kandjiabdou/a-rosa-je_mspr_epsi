{"ast":null,"code":"import liste from \"../data.json\";\nimport { mdiDelete } from \"@mdi/js\";\nimport axios from \"axios\";\nexport default {\n  name: \"liSte\",\n  data: () => ({\n    tab: 0,\n    items: liste[\"liste\"],\n    icon: mdiDelete,\n    gardiennageData: []\n  }),\n  methods: {\n    deleteItem(index) {\n      this.items.splice(index, 1);\n    },\n    fetchData() {\n      console.log(\"test start\");\n      axios.get(\"http://localhost:3000/gardiennage\").then(response => {\n        this.GardiennageData = response.data;\n        console.log(\"ok get\");\n        console.log(response);\n      }).catch(error => {\n        console.log(error);\n      });\n      console.log(\"test fin\");\n\n      // axios.get('http://localhost:3000/gardiennage')\n      //   .then(response => {\n      //     this.gardiennageData = response.data;\n      //     console.log(response.data);\n      //   })\n      //   .catch(error => {\n      //     console.log(error);\n      //   });\n      // const axios = require('axios');\n\n      // let res = await axios.get('http://localhost:3000/gardiennage');\n\n      // let data = res.data;\n      // console.log(data);\n    }\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,KAAI,MAAO,cAAc;AAChC,SAASC,SAAQ,QAAS,SAAS;AACnC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,OAAO;EAEbC,IAAI,EAAE,OAAO;IACXC,GAAG,EAAE,CAAC;IACNC,KAAK,EAAEN,KAAK,CAAC,OAAO,CAAC;IACrBO,IAAI,EAAEN,SAAS;IACfO,eAAe,EAAE;EACnB,CAAC,CAAC;EACFC,OAAO,EAAE;IACPC,UAAU,CAACC,KAAK,EAAE;MAChB,IAAI,CAACL,KAAK,CAACM,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAC7B,CAAC;IACDE,SAAS,GAAG;MACVC,OAAO,CAACC,GAAG,CAAC,YAAY,CAAC;MACzBb,KAAI,CACDc,GAAG,CAAC,mCAAmC,EACvCC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACC,eAAc,GAAID,QAAQ,CAACd,IAAI;QACpCU,OAAO,CAACC,GAAG,CAAC,QAAQ,CAAC;QACrBD,OAAO,CAACC,GAAG,CAACG,QAAQ,CAAC;MACvB,CAAC,EACAE,KAAK,CAAEC,KAAK,IAAK;QAChBP,OAAO,CAACC,GAAG,CAACM,KAAK,CAAC;MACpB,CAAC,CAAC;MACJP,OAAO,CAACC,GAAG,CAAC,UAAU,CAAC;;MAEvB;MACA;MACA;MACA;MACA;MACA;MACA;MACA;MACA;;MAEA;;MAEA;